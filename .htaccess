<ifModule mod_rewrite.c>
RewriteEngine on
RewriteRule ^banana$ views/banana.php
RewriteRule ^Profil$ views/profil.php
RewriteRule ^disconnect$ views/disconnect.php
RewriteRule ^home$ index.php
RewriteRule ^Parameter$ views/parameterAccount.php
RewriteRule ^Ajout_Article$ views/addArticle.php
RewriteRule ^Article$ views/showAllArticle.php
RewriteRule ^Article-Infos-([\d]+)$ views/articleMoreInfo.php?Article=$1
ErrorDocument 404 /views/error404.php
</ifModule>